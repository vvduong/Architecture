@model Architecture.Application.Menus.Queries.GetMenuDetail.MenuDetailModel

@{
    ViewBag.Title = "Menu";
}

<h2>Menu</h2>
<table>
    <tr>
        <th>ID</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Icon</th>
        <td>@Model.Icon</td>
    </tr>
    <tr>
        <th>Url</th>
        <td>@Model.Url</td>
    </tr>
    <tr>
        <th>NoOrder</th>
        <td>@Model.NoOrder</td>
    </tr>
    <tr>
        <th>MenuType</th>
        <td>$@Model.MenuType</td>
    </tr>
</table>

